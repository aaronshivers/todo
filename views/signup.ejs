<% include partials/top %>

<div class="container">

  <h1 class="text-center">Signup Form</h1>

  <form class="form" action="/users" method="POST">
    
    <div class="form-group">
      <label for="email">Email</label>
      <input class="form-control" type="email" name="email" placeholder="Enter Email..." "autocomplete="email" required>
    </div>

    <div class="form-group">
      <label for="password">Pasword</label>
      <input class="form-control" type="password" name="password" placeholder="Enter Password..." "autocomplete="password" required>
    </div>

    <button class="btn btn-outline-primary btn-block">Signup</button>

  </form>

</div>

<% include partials/bottom %>
